<h2>Portfolio Items</h2>
 
<%= link_to "Create new item", new_portfolio_url %>

<% @portfolios_item.each do |portfolio| %>
    <p><%= link_to portfolio.title, portfolio_show_path(portfolio)%></p>
    <p><%= portfolio.subtitle %></p>
    <p><%= portfolio.body %></p>
    <%= image_tag portfolio.main_image  if !portfolio.main_image.nil?  %>

    <%= link_to my-blog-post-0/toggle_status"Edit item", edit_portfolio_path(portfolio.id) %>
    <p>    <%= link_to 'Delete Portfolio item', portfolio_path(portfolio), data: {turbo_method: :delete} %>
    </p>
   
<%end%>